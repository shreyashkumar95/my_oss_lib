  },
  "homepage": "https://github.com/jslvtr98/my_oss_lib#readme",
  "nyc": {
    "all": true,
    "check-coverage": true,
    "branches": 75,
    "functions": 75,
    "lines": 75,
    "statements": 75,
    "reporter": [
      "text",
      "lcov"
    ],
    "include": [
      "src"
    ],
    "sourceMap": "false",
    "instrument": "false",
    "require": [
      "babel-register"
    ]
  },
